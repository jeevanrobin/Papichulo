NODE_ENV=production
PORT=3001
DATABASE_URL="postgresql://user:password@host:5432/papichulo?schema=public"
JWT_SECRET=replace-with-strong-secret
JWT_EXPIRES_IN=7d
ADMIN_API_KEY=replace-with-admin-key
ALLOWED_ORIGINS=https://jeevanrobin.github.io,https://your-domain.com
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX=120
GOOGLE_MAPS_API_KEY=
OTP_PROVIDER=twilio
EXPOSE_DEBUG_OTP=false
OTP_MESSAGE_TEMPLATE="Your Papichulo OTP is {{OTP}}. Valid for {{MINUTES}} minutes."
TWILIO_ACCOUNT_SID=ACxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
TWILIO_AUTH_TOKEN=replace-with-twilio-auth-token
TWILIO_FROM_NUMBER=+1xxxxxxxxxx

